{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "max-len": ["warn", { 
      "code": 120, 
      "ignoreComments": true,
      "ignoreUrls": true,
      "ignoreStrings": true
    }],
    "complexity": ["warn", { "max": 20 }]
  }
}